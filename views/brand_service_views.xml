<odoo>
    <record id="view_brand_service_form" model="ir.ui.view">
        <field name="name">brand.service.form</field>
        <field name="model">brand.service</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="customer_id"/>
                        <field name="product_id"/>
                        <field name="location"/>
                        <field name="action"/>
                        <field name="date"/>
                        <field name="technician_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>